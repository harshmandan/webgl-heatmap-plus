{
	"name": "webgl-heatmap-plus",
	"version": "0.0.6",
	"description": "An optimization of @pyalot excellent WebGL Heatmap project - https://github.com/pyalot/webgl-heatmap",
	"author": "nwaughachukwuma<nwaughac@gmail.com>",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/nwaughachukwuma/webgl-heatmap-plus.git"
	},
	"main": "lib/index.js",
	"type": "module",
	"types": "lib/index.d.ts",
	"sideEffects": false,
	"scripts": {
		"build": "tsc && tsup",
		"prepare": "husky install",
		"pretty-quick": "pretty-quick --staged",
		"tsc": "tsc",
		"test": "tsd && ava"
	},
	"exports": {
		".": {
			"import": "./lib/index.js",
			"require": "./lib/index.cjs"
		}
	},
	"files": [
		"lib"
	],
	"keywords": [
		"webgl",
		"heatmap",
		"webgl-heatmap"
	],
	"devDependencies": {
		"@playcanvas/canvas-mock": "^1.0.1",
		"ava": "^4.0.0",
		"browser-env": "^3.3.0",
		"husky": "^7.0.4",
		"prettier": "^2.5.1",
		"pretty-quick": "^3.1.3",
		"tsd": "^0.19.1",
		"tsup": "^5.11.11",
		"typescript": "^4.5.4"
	},
	"engines": {
		"node": ">=12.0.0"
	},
	"tsd": {
		"directory": "./tsd"
	},
	"ava": {
		"require": [
			"./test/_browser-env.cjs"
		]
	}
}
