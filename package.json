{
	"name": "webgl-heatmap-plus",
	"version": "0.0.3",
	"description": "An optimization of @pyalot excellent WebGL Heatmap project - https://github.com/pyalot/webgl-heatmap",
	"author": "nwaughachukwuma<nwaughac@gmail.com>",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/nwaughachukwuma/ws-rekanet.git"
	},
	"main": "lib/index.js",
	"type": "module",
	"types": "lib/index.d.ts",
	"sideEffects": false,
	"scripts": {
		"build": "tsc && tsup",
		"prepare": "husky install",
		"pretty-quick": "pretty-quick --staged",
		"tsc": "tsc",
		"test": "ava"
	},
	"exports": {
		".": {
			"import": "./lib/index.js",
			"require": "./lib/index.cjs"
		}
	},
	"files": [
		"lib"
	],
	"keywords": [
		"webgl",
		"heatmap",
		"webgl-heatmap"
	],
	"devDependencies": {
		"ava": "^4.0.0",
		"husky": "^7.0.4",
		"prettier": "^2.5.1",
		"pretty-quick": "^3.1.3",
		"tsup": "^5.11.11",
		"typescript": "^4.5.4"
	},
	"engines": {
		"node": ">=12.0.0"
	}
}
